{% extends 'basez1.html.twig' %}

{% block content %}



    <div class="container">
        <h1>Ajouter une Station</h1>

        {{ form_start(form)}}
        <div class="form-group ">
            <label for="desc"  class="control-label">Nom</label>
            <input type="text" class="form-control"  id="nom" rows="4" name="nom" minlength="4" maxlength="300" required >
        </div>
        <div class="form-group ">
            <label for="num"  class="control-label">Longitude</label>
            <input class="form-control"   type="text" id="longitude" name="longitude" required>
        </div>
        <div class="form-group ">
            <label for="num"  class="control-label">Lattitude</label>
            <input class="form-control"   type="text" id="lattitude" name="lattitude" required>
        </div>



        <div class="form-group ">
            {{ include('@Circuits/Circuits/index.html.twig') }}
        </div>

        <div class="form-actions">
            <button type="submit" class="btn btn-success"> <i class="fa fa-check"></i>Ajouter</button>

        </div>

    </div>
    {{ form_end(form)}}

{% endblock %}